{
  "name": "vue3-ts-project-config",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "dev:mock": "vite --host --mode mockdev",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:mock": "vite build --mode mockprod",
    "build:check": "vite build --mode check",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:stylelint": "stylelint  \"**/*.{css,scss,less,vue,html}\" --fix",
    "prepare": "husky install",
    "lint:lint-staged": "lint-staged",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "axios": "^1.7.7",
    "element-plus": "^2.8.6",
    "pinia": "^2.2.4",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@iconify-json/ep": "^1.2.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.17.0",
    "@unocss/eslint-plugin": "^0.62.4",
    "@unocss/preset-rem-to-px": "^0.63.6",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/tsconfig": "^0.5.1",
    "commitizen": "^4.3.1",
    "cz-git": "^1.10.1",
    "eslint": "^9.12.0",
    "eslint-plugin-format": "^0.1.2",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "mockjs": "^1.1.0",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.47",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.80.5",
    "stylelint": "^16.10.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard": "^36.0.1",
    "terser": "^5.36.0",
    "typescript": "~5.6.0",
    "unocss": "^0.63.6",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.10",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-plugin-mock": "2.9.6",
    "vite-plugin-px2rem": "^1.0.4",
    "vite-plugin-restart": "^0.4.1",
    "vite-plugin-vue-devtools": "^7.5.3",
    "vue-tsc": "^2.1.6"
  }
}
